ooter\">\n            <button\n              className=\"modal-close\"\n              onClick={closeModal}\n            >\n              CLOSE\n            </button>\n            {findByKey('footer')}\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default Modal\n","/Users/Kahil/Desktop/codeProjects/chat_app_websockets/client/src/services/chatService.js",[],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":1,"column":8,"nodeType":"119","messageId":"120","endLine":1,"endColumn":12},{"ruleId":"113","replacedBy":"121"},{"ruleId":"115","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":31,"column":11,"nodeType":"125","endLine":31,"endColumn":53},{"ruleId":"113","replacedBy":"126"},{"ruleId":"115","replacedBy":"127"},{"ruleId":"113","replacedBy":"128"},{"ruleId":"115","replacedBy":"129"},{"ruleId":"123","severity":1,"message":"124","line":28,"column":13,"nodeType":"125","endLine":28,"endColumn":55},{"ruleId":"117","severity":1,"message":"130","line":5,"column":3,"nodeType":"119","messageId":"120","endLine":5,"endColumn":17},{"ruleId":"117","severity":1,"message":"131","line":6,"column":3,"nodeType":"119","messageId":"120","endLine":6,"endColumn":16},{"ruleId":"117","severity":1,"message":"132","line":7,"column":3,"nodeType":"119","messageId":"120","endLine":7,"endColumn":17},{"ruleId":"117","severity":1,"message":"133","line":8,"column":3,"nodeType":"119","messageId":"120","endLine":8,"endColumn":13},{"ruleId":"117","severity":1,"message":"134","line":9,"column":3,"nodeType":"119","messageId":"120","endLine":9,"endColumn":19},{"ruleId":"117","severity":1,"message":"135","line":10,"column":3,"nodeType":"119","messageId":"120","endLine":10,"endColumn":16},{"ruleId":"117","severity":1,"message":"136","line":11,"column":3,"nodeType":"119","messageId":"120","endLine":11,"endColumn":20},{"ruleId":"117","severity":1,"message":"137","line":12,"column":3,"n